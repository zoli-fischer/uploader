{
  "name": "@zoli-fischer/uploader",
  "version": "1.0.7",
  "description": "JavaScript class for handling file/data upload",
  "author": "zoli-fischer",
  "homepage": "https://github.com/zoli-fischer/uploader#readme",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "jsnext:main": "lib/index.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env mocha --require babel-core/register --colors -w ./src/test.js",
    "dev": "cross-env WEBPACK_ENV=dev webpack --progress --colors --watch --config webpack.config.js",
    "build": "cross-env WEBPACK_ENV=build webpack --config webpack.config.js",
    "publish-public": "cross-env npm publish --access public",
    "publish-dry": "cross-env npm publish --access public --dry-run",
    "predeploy": "cross-env cd example && npm install && npm run build",
    "deploy": "cross-env gh-pages -d example/build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zoli-fischer/uploader.git"
  },
  "keywords": [
    "uploader",
    "react",
    "javascript"
  ],
  "bugs": {
    "url": "https://github.com/zoli-fischer/uploader/issues"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.2.0",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "expect": "^23.6.0",
    "gh-pages": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^5.2.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.9"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "eslint-plugin-filenames": "^1.3.2"
  }
}
